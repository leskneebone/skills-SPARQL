@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dewr: <https://linked.data.gov.au/org/dewr> .
@prefix jsa: <https://linked.data.gov.au/org/jsa> .
@prefix nsto: <https://test.linked.data.gov.au/def/nst/ontology#> .
@prefix sdi: <https://test.linked.data.gov.au/def/nst-digital-intensity/> .
@prefix sdics: <https://test.linked.data.gov.au/def/nst-digital-intensity> .
@prefix percentrange: <https://test.linked.data.gov.au/def/nst-percent-range/> .
@prefix percentrangecs: <https://test.linked.data.gov.au/def/nst-percent-range> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

jsa:
a schema:Organization ;
schema:name "Jobs and Skills Australia"@en ;
schema:url "https://wwww.jobsandskills.gov.au"^^xsd:anyURI
.

dewr:
a schema:Organization ;
schema:name "Department of Employment and Work Relations"@en ;
schema:url "https://wwww.dewr.gov.au"^^xsd:anyURI
.

sdics: a skos:ConceptScheme ;
dcterms:created "2026-02-05"^^xsd:date ;
dcterms:modified "2026-02-13"^^xsd:date ;
owl:versionIRI <https://test.linked.data.gov.au/def/nstdi/0.0.1> ;
skos:prefLabel "Skill Digital Intensity"@en ;
skos:definition "Classification indicating extent of digital technology use required"@en ;
skos:creator jsa: ;
skos:publisher dewr: ;
skos:hasTopConcept 
  sdi:MIN ,
  sdi:LOW ,
  sdi:MED ,
  sdi:HIG ,
  sdi:ADV
.

sdi:MIN a skos:Concept ;
skos:inScheme sdics: ;
skos:topConceptOf sdics: ;
skos:notation "0" ;
skos:editorialNote "Digital intensity is 0-10%"@en ;
nsto:digitalIntensity percentrange:_0_10 ;
skos:definition "Tasks mostly manual with minimal digital involvement"@en ;
skos:prefLabel "Minimal Digital"@en
.

sdi:LOW a skos:Concept ;
skos:inScheme sdics: ;
skos:topConceptOf sdics: ;
skos:notation "1" ;
skos:editorialNote "Digital intensity is 11-30%"@en ;
nsto:digitalIntensity percentrange:_11_30 ;
skos:definition "Basic digital literacy and simple tools"@en ;
skos:prefLabel "Low Digital"@en
.

sdi:MED a skos:Concept ;
skos:inScheme sdics: ;
skos:topConceptOf sdics: ;
skos:notation "2" ;
skos:editorialNote "Digital intensity is 31-60%"@en ;
nsto:digitalIntensity percentrange:_31_60 ;
skos:definition "Frequent use of specialized software and platforms"@en ;
skos:prefLabel "Medium Digital"@en
.

sdi:HIG a skos:Concept ;
skos:inScheme sdics: ;
skos:topConceptOf sdics: ;
skos:notation "3" ;
skos:editorialNote "Digital intensity is 60-85%"@en ;
nsto:digitalIntensity percentrange:_61_85 ;
skos:definition "Advanced digital skills, complex software"@en ;
skos:prefLabel "High Digital"@en
.

sdi:ADV a skos:Concept ;
skos:inScheme sdics: ;
skos:topConceptOf sdics: ;
skos:notation "4" ;
skos:editorialNote "Digital intensity is 86-100%"@en ;
nsto:digitalIntensity percentrange:_86_100 ;
skos:definition "Expertise in cutting-edge technologies"@en ;
skos:prefLabel "Advanced Digital"@en
.

percentrangecs: a skos:ConceptScheme ;
skos:prefLabel "Percentage ranges"@en ;
skos:definition "Percentage ranges applied to skill attributes, including the digital intensity of a skill"@en ;
skos:creator jsa: ;
skos:publisher dewr: ;
skos:hasTopConcept 
  percentrange:_0_10 ,
  percentrange:_11_30 ,
  percentrange:_31_60 ,
  percentrange:_61_85 ,
  percentrange:_86_100
.

percentrange:_0_10 a skos:Concept ;
skos:inScheme percentrangecs: ;
skos:topConceptOf percentrangecs: ;
skos:prefLabel "0-10%"@en ;
schema:minVal "0"^^xsd:integer ;
schema:maxVal "10"^^xsd:integer .

percentrange:_11_30 a skos:Concept ;
skos:inScheme percentrangecs: ;
skos:topConceptOf percentrangecs: ;
skos:prefLabel "11-30%"@en ;
schema:minVal "11"^^xsd:integer ;
schema:maxVal "30"^^xsd:integer .

percentrange:_31_60 a skos:Concept ;
skos:inScheme percentrangecs: ;
skos:topConceptOf percentrangecs: ;
skos:prefLabel "31-60%"@en ;
schema:minVal "31"^^xsd:integer ;
schema:maxVal "60"^^xsd:integer .

percentrange:_61_85 a skos:Concept ;
skos:inScheme percentrangecs: ;
skos:topConceptOf percentrangecs: ;
skos:prefLabel "61-85%"@en ;
schema:minVal "61"^^xsd:integer ;
schema:maxVal "85"^^xsd:integer .

percentrange:_86_100 a skos:Concept ;
skos:inScheme percentrangecs: ;
skos:topConceptOf percentrangecs: ;
skos:prefLabel "86-100%"@en ;
schema:minVal "86"^^xsd:integer ;
schema:maxVal "100"^^xsd:integer .

nsto:digitalIntensity a rdf:Property ;
rdfs:label "Digital Intensity" ;
rdfs:comment "Property indicating the digtial intensity of a Skill, indicated with a percentage range"
.

schema:minVal a rdf:Property ;
rdfs:label "MinVal" .

schema:maxVal a rdf:Property ;
rdfs:label "MaxVal" .
