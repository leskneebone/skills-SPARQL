  skos:inScheme digcompcs:;
  skos:prefLabel "Interacting through and with digital technologies"@en;
  skos:altLabel "Interacting through digital technologies"@en;
  skos:definition "To interact through and with a variety of digital technologies, and to use appropriate digital communication for a given context."@en ;
  skos:notation "2.1";
  dcterms:modified "2025-03-03T05:46:44.689Z"^^xsd:dateTime;
