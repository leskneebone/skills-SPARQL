  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:23:47.973Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Sharing through digital technologies"@en;
  skos:definition "To share information and content ethically and responsibly with others through appropriate digital technologies."@en ;
  skos:notation "2.2";
  dcterms:modified "2025-03-03T05:46:51.648Z"^^xsd:dateTime;
  skos:broader digcomp:ded05862-dc66-4564-9223-890481316a44 .

digcomp:22191328-30b0-4b90-8ddb-c49d38a44cd2
  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:24:06.313Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Engaging in citizenship through digital technologies"@en;
  skos:definition "To participate in society through the ethical and responsible use of digital platforms and services. To seek opportunities for self-empowerment and participation through appropriate digital technologies. To be aware of and assert one’s rights, and to exercise choice, in digital environments."@en ;
  skos:notation "2.3";
  dcterms:modified "2025-03-03T05:46:42.515Z"^^xsd:dateTime;
  skos:broader digcomp:ded05862-dc66-4564-9223-890481316a44 .

digcomp:d6bb5cf7-1480-402a-9eb3-a511eb4cb945
  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:24:18.416Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Collaborating through digital technologies"@en;
  skos:definition "To use digital technologies ethically and responsibly for collaborative purposes, and for the co-construction and co-creation of information, resources and knowledge."@en ;
  skos:notation "2.4";
  dcterms:modified "2025-03-03T05:46:40.470Z"^^xsd:dateTime;
  skos:broader digcomp:ded05862-dc66-4564-9223-890481316a44 .

digcomp:43d68b7a-6bcf-4e2f-bc14-4f601bb73fb0
  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:24:25.906Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Digital behaviour"@en;
  skos:altLabel "Netiquette"@en;
  skos:definition "To be aware of behavioural norms, and to know how to behave respectfully while using digital technologies and interacting in digital environments. To adapt communication to specific contexts, and to be aware of and respect cultural, generational and other diversity in digital environments."@en ;
  skos:notation "2.5";
  dcterms:modified "2025-03-03T05:46:49.530Z"^^xsd:dateTime;
  skos:broader digcomp:ded05862-dc66-4564-9223-890481316a44 .
  
  digcomp:135ca7d8-bf57-44e8-ba27-0016a0871dda
  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:24:36.838Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Managing digital identity"@en;
  skos:definition "To manage one or multiple digital identities. To take actions to help protect one’s digital reputation (how one is perceived based on online presence), and to manage one’s digital footprint (the data that is produced through use of and by digital platforms and services)."@en ;
  skos:notation "2.6";
  dcterms:modified "2025-03-03T05:46:47.340Z"^^xsd:dateTime;
  skos:broader digcomp:ded05862-dc66-4564-9223-890481316a44 .

