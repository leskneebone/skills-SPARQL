  a skos:Concept , esco:Skill ;
  dcterms:created "2025-02-25T23:26:32.363Z"^^xsd:dateTime;
  dcterms:source "https://joint-research-centre.ec.europa.eu/scientific-activities-z/education-and-training/digital-transformation-education/digital-competence-framework-citizens-digcomp_en"^^xsd:anyURI  ;
  skos:inScheme digcompcs:;
  skos:prefLabel "Developing digital content"@en;
  skos:definition "To use digital technologies ethically and responsibly to create and edit a variety of content. To express oneself through digital means."@en ;
  skos:notation "3.1";
  dcterms:modified "2025-03-03T05:47:00.468Z"^^xsd:dateTime;
