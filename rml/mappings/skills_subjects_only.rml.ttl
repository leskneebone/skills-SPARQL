@base <http://example.com/rml/> .
@prefix asced: <https://abs.gov.au/classifications/ASCED/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix esco: <http://data.europa.eu/esco/model#> .
@prefix nsto: <https://test.linked.data.gov.au/def/nst/ontology#> .
@prefix rr:  <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:  <http://semweb.mmlab.be/ns/ql#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<#SkillTriplesMap>
  a rr:TriplesMap ;

  rml:logicalSource [
    rml:source "source/faceted_taxonomy_with_qual_occ.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://test.linked.data.gov.au/def/nst/{skill_id}" ;
    rr:class skos:Concept
  ] ;

  rr:predicateObjectMap [
    rr:predicate skos:inScheme ;
    rr:objectMap [
      rr:constant <https://test.linked.data.gov.au/def/nst>
    ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate skos:prefLabel ;
    rr:objectMap [
      rml:reference "skill_name" ;
      rr:language "en"
    ]
  ] ;

rr:predicateObjectMap [
    rr:predicate skos:definition ;
    rr:objectMap [
      rml:reference "descripition" ;
      rr:language "en"
    ]
  ] ;

rr:predicateObjectMap [
    rr:predicate skos:notation ;
    rr:objectMap [
      rml:reference "skill_id" ;
    ]
  ] .

